<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape>
			<padding android:top="2px" />
			<solid android:color="#8A3324" />
			<corners android:bottomRightRadius="5dp"
				android:bottomLeftRadius="5dp" android:topLeftRadius="5dp"
				android:topRightRadius="5dp" />
		</shape>
	</item>
	<item>
		<shape>
			<padding android:bottom="2px" />
			<solid android:color="#41090b" />
			<corners android:bottomRightRadius="5dp"
				android:bottomLeftRadius="5dp" android:topLeftRadius="5dp"
				android:topRightRadius="5dp" />
		</shape>
	</item>
	<item>
		<shape>
			<padding android:top="2px" android:bottom="2px" />
			<gradient android:startColor="#600E11" android:endColor="#532104"
				android:type="linear" android:angle="270" android:dither="true" />
			<corners android:bottomRightRadius="5dp"
				android:bottomLeftRadius="5dp" android:topLeftRadius="5dp"
				android:topRightRadius="5dp" />
		</shape>
	</item>
</layer-list>